{
  "name": "ortelius",
  "version": "0.0.1-beta.7",
  "description": "a tool for making good-looking maps",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "webpack-dev-server --mode=development",
    "eslint": "eslint --ext js,jsx src",
    "build-dist": "webpack --mode=production",
    "build-lib": "babel src -d lib",
    "build": "npm run build-dist && npm run build-lib",
    "clean": "rimraf dist lib",
    "depcheck": "david",
    "todo": "leasot -x --reporter markdown 'src/**/*' > TODO.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laurencedorman/ortelius.git"
  },
  "keywords": [
    "maps",
    "cartography",
    "geojson",
    "topojson",
    "dataviz",
    "d3"
  ],
  "author": "laurencedorman",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/laurencedorman/ortelius/issues"
  },
  "homepage": "https://github.com/laurencedorman/ortelius#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "d3-dsv": "^1.1.1",
    "d3-format": "^1.3.2",
    "d3-geo": "^1.11.3",
    "d3-scale": "^3.0.0",
    "d3-scale-chromatic": "^1.3.3",
    "dayjs": "^1.8.14",
    "prop-types": "^15.7.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-spring": "^8.0.5",
    "topojson-client": "^3.0.0",
    "topojson-simplify": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "css-loader": "^3.0.0",
    "david": "^11.0.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "leasot": "^7.3.4",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
